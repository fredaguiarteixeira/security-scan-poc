{
  "name": "portal-starter-template-ui",
  "version": "1.0.1",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build:dev": "export NODE_ENV=development && webpack --config webpack.dev.js --verbose --display-error-details",
    "build:staging": "NODE_OPTIONS=--max-old-space-size=768 export NODE_ENV=staging && webpack --config webpack.staging.js",
    "build:production": "NODE_OPTIONS=--max-old-space-size=768 export NODE_ENV=production && webpack --config webpack.prod.js",
    "build": "npm run build:staging && npm run build:production",
    "debug": "node --inspect-brk=9229 dist/server/server.js",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "progress": "webpack -p --progress",
    "server:dev": "node dist/development/server/server.js",
    "server:production": "node dist/production/server/server.js",
    "server:staging": "node dist/staging/server/server.js",
    "server": "if test \"$NODE_ENV\" = \"production\" ; then npm run server:production;elif test \"$NODE_ENV\" = \"staging\" ; then npm run server:staging; else npm run server:dev; fi;",
    "test": "npm run lint && npm run unit",
    "unit:debug": "node --inspect-brk=9229 ./node_modules/jest/bin/jest.js --verbose --runInBand",
    "unit": "jest --verbose --runInBand",
    "validate": "npm run lint && npm run prettier -- --list-different"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@tds/core-box": "^1.2.4",
    "@tds/core-button": "^2.1.6",
    "@tds/core-css-reset": "^1.2.1",
    "@tds/core-heading": "^2.2.5",
    "@tds/core-input": "^3.0.19",
    "@tds/core-link": "^2.0.9",
    "@tds/core-spinner": "^2.2.2",
    "@tds/core-text": "^1.1.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.2",
    "express-session": "^1.16.1",
    "helmet": "^3.21.2",
    "memory-cache": "^0.2.0",
    "morgan": "^1.9.0",
    "passport": "0.4.x",
    "passport-saml": "0.14.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-flexbox-grid": "^2.1.2",
    "react-intl": "^3.2.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "audit-ci": "^2.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.5.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.0",
    "jest": "^22.2.2",
    "jest-localstorage-mock": "^2.4.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.16.4",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
